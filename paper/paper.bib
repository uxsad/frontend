@article{Larusdottir2023UCD,
  title = {{{UCD Sprint}}: {{A Fast Process}} to {{Involve Users}} in the {{Design Practices}} of {{Software Companies}}},
  shorttitle = {{{UCD Sprint}}},
  author = {Larusdottir, Marta K. and Lanzilotti, Rosa and Piccinno, Antonio and Visescu, Ioana and Costabile, Maria Francesca},
  year = {2023},
  month = nov,
  journal = {International Journal of Human--Computer Interaction},
  pages = {1--18},
  issn = {1044-7318, 1532-7590},
  doi = {10.1080/10447318.2023.2279816},
  url = {https://www.tandfonline.com/doi/full/10.1080/10447318.2023.2279816},
  urldate = {2023-12-07},
  abstract = {Several studies have shown that involving users in design processes is a key factor in understand\- ing user needs and essential for designing computer systems capable of providing valuable user experiences. However, many practitioners do not emphasize user needs sufficiently and neglect user involvement in software design and development. The UCD Sprint is a recently proposed, step-by-step, cost-effective process that focuses on involving users in design activities. This article reports a mixed-method study in which, for the first time, practitioners working in companies used the UCD Sprint; the study's main objective was to investigate the impact of the UCD Sprint on practitioners' design practices. The results show that, among the various activities associated with this process, those that involved users were particularly appreciated by practitioners. Thus, the UCD Sprint can enhance the innovativeness of company design practices; it enables practi\- tioners to obtain sufficient information to create systems that fit users' needs.},
  langid = {english},
  keywords = {notion},
  file = {/Users/andrea/Zotero/storage/DEIKUIUU/UCD Sprint  A Fast Process to Involve Users in the Design Practices of Software Companies.epub;/Users/andrea/Zotero/storage/VD76U7D5/Larusdottir et al. - 2023 - UCD Sprint A Fast Process to Involve Users in the.pdf}
