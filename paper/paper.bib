@mastersthesis{Greco2022Explainable,
  title = {An {{Explainable AI Model}} to {{Help Users Avoid Being Victims}} of {{Phishing Attacks}}},
  author = {Greco, Francesco},
  year = {2022},
  month = jul,
  abstract = {One of the greatest cyber threats of our time is phishing, a cybercrime in which criminals try to steal personal information from victims with different techniques, e.g., by using malicious emails. Warning messages are a useful tool in combating phishing attacks, but they often lack information about why a specific email is considered dangerous. In this thesis, we present the design and development of an eXplainble Artificial Intelligence tool for phishing detection that classifies emails as phishing/non-phishing, also showing users explanations about the causes that led to the classification of a specific mail as suspicious. To be interpretable to lay users, a selection of features that the ML model uses for the classification has been performed to explain only features that do not require IT knowledge. Different state-of-the-art ML and AI models have been trained and tested on publicly available datasets of phishing emails. While interpretable models (such as decision tree and logistic regression classifiers) are explainable by-design, to explain black-box models we have employed the most common post-hoc explanation mechanisms.},
  collaborator = {Desolda, Giuseppe},
  school = {University of Bari Aldo Moro},
  file = {/Users/andrea/Zotero/storage/5KYKF7ZL/Greco Francesco Masters.pdf}
}

