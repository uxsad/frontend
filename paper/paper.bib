@article{Cuadra2021My,
  title = {My Bad! {{Repairing}} Intelligent Voice Assistant Errors Improves Interaction},
  author = {Cuadra, Andrea and Li, Shuran and Lee, Hansol and Cho, Jason and Ju, Wendy},
  year = {2021},
  month = apr,
  journal = {Proc. ACM Hum.-Comput. Interact.},
  volume = {5},
