  shorttitle = {Usability Testing of a Healthcare Chatbot},
  booktitle = {Proceedings of the 31st {{European Conference}} on {{Cognitive Ergonomics}}},
  author = {Holmes, Samuel and Moorhead, Anne and Bond, Raymond and Zheng, Huiru and Coates, Vivien and Mctear, Michael},
  year = {2019},
