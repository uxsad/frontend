@inproceedings{Huang2017Braincomputer,
  title = {Brain-Computer Interfaces ({{BCI}}) Based {{3D}} Computer-Aided Design ({{CAD}}): {{To}} Improve the Efficiency of {{3D}} Modeling for New Users},
