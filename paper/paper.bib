@article{Holzinger2022Personas,
  type = {Article},
  title = {Personas for {{Artificial Intelligence}} ({{AI}}) an {{Open Source Toolbox}}},
  author = {Holzinger, Andreas and Kargl, Michaela and Kipperer, Bettina and Regitnig, Peter and Plass, Markus and Muller, Heimo},
  year = {2022},
  journal = {IEEE Access},
  volume = {10},
